<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.TextArea?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.control.Tooltip?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.text.Font?>

<AnchorPane prefHeight="400.0" prefWidth="400.0" xmlns="http://javafx.com/javafx/8.0.171" xmlns:fx="http://javafx.com/fxml/1" fx:controller="application.MainController">
   <children>
      <Label layoutX="104.0" layoutY="10.0" prefHeight="60.0" prefWidth="154.0" text="Example4">
         <font>
            <Font name="System Bold" size="31.0" />
         </font>
      </Label>
      <Button fx:id="_btnAccept" layoutX="72.0" layoutY="331.0" mnemonicParsing="false" onAction="#onAcceptClicked" text="Accept">
         <tooltip>
            <Tooltip text="Click to Accept." />
         </tooltip></Button>
      <Button fx:id="_btnReject" layoutX="155.0" layoutY="331.0" mnemonicParsing="false" onAction="#onRejectClicked" text="Reject">
         <tooltip>
            <Tooltip text="Click to reject." />
         </tooltip></Button>
      <TextArea fx:id="_txtOutput" editable="false" layoutX="47.0" layoutY="197.0" prefHeight="109.0" prefWidth="274.0" />
      <Button fx:id="_btnClear" layoutX="247.0" layoutY="331.0" mnemonicParsing="false" onAction="#onClearClicked" text="Clear" />
      <TextField fx:id="_txtInput" layoutX="51.0" layoutY="80.0" promptText="Enter a number" />
      <Button fx:id="_btnSubmit" layoutX="232.0" layoutY="80.0" mnemonicParsing="false" onAction="#onSubmitClicked" text="Submit" />
      <Label fx:id="_lblError" layoutX="51.0" layoutY="123.0" prefHeight="17.0" prefWidth="245.0" textFill="#eb1b1b" />
   </children>
</AnchorPane>
